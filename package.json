{
	"name": "trpc-v10-migrate-codemod",
	"version": "1.3.2",
	"type": "module",
	"description": "codemod to migrate your tRPC codebase from v9 to v10",
	"repository": {
		"type": "git",
		"url": "https://github.com/sachinraja/trpc-v10-migrate-codemod.git"
	},
	"homepage": "https://github.com/sachinraja/trpc-v10-migrate-codemod#readme",
	"bugs": {
		"url": "https://github.com/sachinraja/trpc-v10-migrate-codemod/issues"
	},
	"author": "Sachin Raja <sachinraja2349@gmail.com>",
	"license": "MIT",
	"bin": "dist/bin.js",
	"main": "dist/index.cjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.cjs"
		}
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"format": "dprint fmt",
		"lint": "run-p lint:*",
		"lint:format": "dprint check",
		"lint:types": "tsc",
		"lint:js": "eslint .",
		"prepublishOnly": "pnpm build",
		"test": "vitest",
		"migrate-v10": "tsx src/bin.ts"
	},
	"dependencies": {
		"clipanion": "3.2.1",
		"lodash-es": "^4.17.21",
		"ts-morph": "24.0.0",
		"typanion": "^3.14.0"
	},
	"devDependencies": {
		"@sachinraja/eslint-config": "0.3.0",
		"@trpc/server": "9.27.2",
		"@types/lodash-es": "^4.17.8",
		"@types/node": "20.4.10",
		"dprint": "0.40.2",
		"eslint": "8.47.0",
		"npm-run-all": "4.1.5",
		"tsup": "7.2.0",
		"tsx": "3.12.7",
		"typescript": "5.1.6",
		"vite": "4.4.9",
		"vitest": "0.34.1",
		"zod": "3.21.4"
	},
	"sideEffects": false,
	"tsup": {
		"entry": [
			"src/index.ts",
			"src/bin.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"dts": {
			"resolve": true
		},
		"splitting": true,
		"clean": true
	},
	"packageManager": "pnpm@8.6.12"
}
